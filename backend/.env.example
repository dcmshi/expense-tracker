# Copy this file to .env and fill in values for your environment

# PostgreSQL connection string
DATABASE_URL="postgresql://user:password@localhost:5432/expense_tracker?schema=public"

# S3-compatible object storage
S3_ENDPOINT="http://localhost:9000"        # MinIO local | remove for AWS S3
S3_BUCKET="expense-tracker-receipts"
S3_REGION="us-east-1"
S3_ACCESS_KEY_ID="your-access-key"
S3_SECRET_ACCESS_KEY="your-secret-key"
S3_PRESIGNED_URL_TTL_SECONDS=300           # 5 minutes

# Google Vision API
GOOGLE_VISION_API_KEY="your-api-key"

# Processing worker
WORKER_POLL_INTERVAL_MS=5000               # how often worker checks for new jobs
WORKER_MAX_ATTEMPTS=3                      # max retries before marking job as failed
